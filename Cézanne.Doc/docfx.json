{
  "build": {
    "output": "_site",
    "content": [
      {
        "files": "**/*.{md,yml}",
        "exclude": "**/_site/**"
      }
    ],
    "resource": [
      {
        "files": "**/images/**"
      },
      {
        "files": "**/generated/**/*.json"
      }
    ],
    "globalMetadata": {
      "_appTitle": "Cézanne",
      "_appName": "Cézanne",
      "_enableSearch": true,
      "_appFooter": "<span>Made with love by <a href=\"https://x.com/rmannibucau\" target=\"_blank\">@rmannibucau</a> using docfx.</span>",
      "pdf": false
    },
    "sitemap": {
      "baseUrl": "http://localhost:8080",
      "priority": 0.1,
      "changefreq": "daily"
    },
    "template": [
      "default"
    ]
  }
}
